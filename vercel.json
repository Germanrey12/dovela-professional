{
  "version": 2,
  "builds": [
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/public/favicon.svg" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "^/index.html$", "dest": "/public/index.html" }
  ],
  "cleanUrls": true
}
